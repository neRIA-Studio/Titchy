{
	"name": "titchy",
	"version": "0.0.10",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"build-style": "sass \"./src/others/global.scss\" \"./dist/styles/global.css\" --no-source-map --style=compressed",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && npm run build-style && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/components/index.d.ts",
			"svelte": "./dist/components/index.js"
		},
		"./styles": {
			"default": "./dist/styles/global.css"
		}
	},
	"peerDependencies": {
		"@lucide/svelte": "^0.545.0",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@lucide/svelte": "^0.545.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"publint": "^0.3.13",
		"sass": "^1.93.2",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"typescript": "^5.9.2",
		"vite": "^7.1.7"
	},
	"keywords": [
		"svelte",
		"components",
		"ui",
		"ui-kit"
	]
}
